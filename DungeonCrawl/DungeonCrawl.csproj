        <RootNamespace>DungeonCrawl</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <Using Include="SadConsole" />
        <Using Include="SadRogue.Primitives" />
        <Using Include="SadConsole.Console" Alias="Console" />
